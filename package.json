{
	"name": "@dxdns/feflow-cli",
	"version": "0.0.4",
	"description": "init projects with @dxdns/feflow",
	"private": false,
	"author": "Di√≥genes Rodrigues <https://github.com/dxdns>",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"homepage": "https://feflow.dxdns.dev",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dxdns/feflow.git"
	},
	"scripts": {
		"build": "tsc",
		"feflow": "npm run build && node ./dist/index.js",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"release": "npm run build && git push && git push --tags",
		"release:patch": "npm version patch && npm run release",
		"release:minor": "npm version minor && npm run release",
		"release:major": "npm version major && npm run release"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"bin": {
		"feflow": "./dist/index.js"
	},
	"type": "module",
	"devDependencies": {
		"prettier": "^3.4.2",
		"@types/node": "^24.0.1",
		"@types/prompts": "^2.4.9",
		"typescript": "^5.1.6"
	},
	"dependencies": {
		"chalk": "^5.4.1",
		"prompts": "^2.4.2"
	},
	"keywords": [
		"cli",
		"feflow",
		"ui",
		"svelte",
		"vite",
		"dxdns"
	]
}
